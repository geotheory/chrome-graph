{
  "name": "ChromeGraph",
  "version": "1",
  "manifest_version" : 2,
  "description": "This extension wants to add pretty network graph to a third-party webpage",
  "background" : { "scripts" : ["background.js"] },
  "browser_action": { "default_icon": "icon.png" },
  "permissions": ["activeTab", "webRequest", "<all_urls>", "https://unpkg.com/*"],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://unpkg.com; object-src 'self'"
}
